<?xml version="1.0" ?>
<autofs_ldap_sasl_conf
  usetls="<%= @usetls %>"
  tlsrequired="<%= @tlsrequired %>"
  authrequired="<%= @authrequired %>"
  authtype="<%= @authtype.upcase %>"
<% if @authtype.upcase.eql?('EXTERNAL') then -%>
  external_cert="<%= @external_cert %>"
  external_key="<%= @external_key %>"
<% end -%>
<% if @user then -%>
  user="<%= @user %>"
<% end -%>
<% if @secret then -%>
  secret="<%= @secret %>"
<% end -%>
<% if @clientprinc then -%>
  clientprinc="<%= @clientprinc %>"
<% end -%>
<% if @credentialcache then -%>
  credentialcache="<%= @credentialcache %>"
<% end -%>
/>
